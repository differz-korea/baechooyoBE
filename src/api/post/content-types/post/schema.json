{
  "kind": "collectionType",
  "collectionName": "posts",
  "info": {
    "singularName": "post",
    "pluralName": "posts",
    "displayName": "Post",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "content": {
      "type": "text",
      "required": true
    },
    "writer": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user"
    },
    "comments": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::comment.comment",
      "mappedBy": "post"
    },
    "type": {
      "type": "enumeration",
      "enum": ["free", "qna", "branch", "job", "direct-transaction"],
      "required": true
    },
    "views": {
      "type": "integer",
      "default": 0
    },
    "name": {
      "type": "string"
    },
    "price": {
      "type": "integer"
    },
    "salary": {
      "type": "integer"
    },
    "location": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::location.location"
    }
  }
}
