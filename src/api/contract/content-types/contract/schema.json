      "type": "boolean",
      "default": true
    },
    "responderAgreement": {
      "type": "boolean"
    },
    "editedAt": {
      "type": "datetime"
    },
    "approvedAt": {
      "type": "datetime"
    },
    "deniedAt": {
      "type": "datetime"
    },
    "expirationDate": {
      "type": "date"
    },
    "validityStatus": {
      "type": "enumeration",
